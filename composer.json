{
  "name": "alexchitoraga/apiok",
  "description": "APIOK PHP SDK with support for Laravel integration",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "ok",
    "sdk",
    "php",
    "laravel",
    "test",
    "api"
  ],
  "authors": [
    {
      "name": "Alexandru Chitoraga",
      "email": "alexandru.chitoraga@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Alexchitoraga\\Apiok\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "Alexchitoraga\\Apiok\\ApiokServiceProvider"
      ],
      "aliases": {
        "Apiok": "Alexchitoraga\\Apiok\\Facades\\Apiok"
      }
    }
  }
}
